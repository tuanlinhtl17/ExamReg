<tr id="ca_thi_<%= ca_thi.id %>">
  <td><%= ca_thi.ten %></td>
  <td><%= ca_thi.ngay_thi %></td>
  <td><%= ca_thi.bat_dau.strftime("%H:%M") %></td>
  <td><%= ca_thi.ket_thuc.strftime("%H:%M") %></td>
  <td><%= ca_thi.mon_thi.hoc_phan.ten %></td>
  <td><%= ca_thi.phong_may.ten %></td>
  <td>
    <%= link_to edit_admin_ky_thi_ca_thi_path(id: ca_thi.id, ky_thi_id: ca_thi.ky_thi_id), remote: true do %>
      <i class="fa fa-lg fa-pencil-square-o" aria-hidden="true"></i>
    <% end %>
    <%= link_to admin_ky_thi_ca_thi_path(id: ca_thi.id, ky_thi_id: ca_thi.ky_thi_id), method: :delete, data: { confirm: "Bạn chắc chắn muốn xóa ?" }, remote: true do %>
      <i class="fa fa-lg fa-trash-o" aria-hidden="true"></i>
    <% end %>
  </td>
</tr>
