<% if @error %>
  toastr.error('<%= @error %>');
<% else %>
  if ($('#duDieuKiensImport .dataTables_empty')) {
    $('#duDieuKiensImport .dataTables_empty').parent().remove()
  }
  $('#duDieuKiensImport').DataTable().row.add($('<%= escape_javascript(render(partial: "admin/du_dieu_kiens_imports/du_dieu_kiens_import", locals: { du_dieu_kiens_import: @du_dieu_kiens_import_new })) %>')[0]).draw()
  toastr.success("Đang import dữ liệu")
<% end %>
