$("#themHocPhan").modal("hide");
<% if !@hoc_phan_new.errors.messages.empty? %>
  $('main').prepend('<%= escape_javascript(render(partial: "shared/error_messages", locals: { object: @hoc_phan_new })) %>')
<% else %>
  if ($('#hocPhan .dataTables_empty')) {
    $('#hocPhan .dataTables_empty').parent().remove()
  }
  $('#new_hoc_phan').trigger('reset');
  $('#hocPhan').DataTable().row.add($('<%= escape_javascript(render(partial: "admin/hoc_phans/hoc_phan", locals: { hoc_phan: @hoc_phan_new })) %>')[0]).draw()
  toastr.success("Thêm học phần thành công")
<% end %>
