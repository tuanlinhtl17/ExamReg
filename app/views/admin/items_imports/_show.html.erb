<div class="modal" id="showItemsImport">
  <div class="modal-dialog">
    <div class="modal-content">

      <!-- Modal Header -->
      <div class="modal-header">
        <h4 class="modal-title">Thông tin file import</h4>
        <button type="button" class="close" data-dismiss="modal">&times;</button>
      </div>

      <!-- Modal body -->
      <div class="modal-body">
          <table id="itemsImport" class="table table-striped table-bordered text-center">
            <thead>
              <tr>
                <th>Dòng</th>
                <th>Lỗi xảy ra</th>
              </tr>
            </thead>
            <tbody>
              <% @items_import.sub_items_errors.each do |sub_items_error| %>
                <tr>
                  <td><%= sub_items_error[:line] %></td>
                  <td><%= sub_items_error[:message] %></td>
                </tr>
              <% end %>
            </tbody>
          </table>
        <div class="clearfix"> </div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-danger" data-dismiss="modal">Đóng</button>
      </div>
    </div>
  </div>
</div>
